{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "typescript-axios": {
        "generatorName": "typescript-axios",
        "output": "./src/generated/api",
        "glob": "**/*",
        "additionalProperties": {
          "supportsES6": true,
          "npmName": "@/generated/api",
          "stringEnums": true,
          "enumPropertyNaming": "PascalCase"
        }
      }
    }
  }
}